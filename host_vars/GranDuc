## Ansible configuration for connecting to remote host
# IP address
ansible_ssh_host: "{{ GranDuc_srv_ip }}"
# User
host_user: "{{ GranDuc_srv_user }}"
# User password
host_password: "{{ GranDuc_srv_pass }}"
# Root password
host_root_password: "{{ GranDuc_srv_root_pass }}"

## Docker configuration
# Location of dockers
docker_location: "/home/docker/"

